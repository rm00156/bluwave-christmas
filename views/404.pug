doctype html
head
    include common/revised/normalHeader.pug
    include common/revised/navbar.pug
    include common/revised/footer.pug
    +head("Page Not Found")
    link(href='assets2/css/style.css' rel='stylesheet')    
body
                                               
    +navbar()

    section.section.mt-6
        // Content
        .container
            .row
                .col-12.col-md-3.offset-md-1.order-md-3
                    // Image
                    .text-center
                        svg.error-img.img-fluid.mb-5.mb-md-0(width='1200px' height='1200px' viewbox='0 0 120 120' xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink')
                            // Body
                            path.error-img-body(d='M20 30 H 2.5 A 2.5 2.5 0 0 0 0 32.5 V 77.5 A 2.5 2.5 0 0 0 2.5 80 H 117.5 A 2.5 2.5 0 0 0 120 77.5 V 32.5 A 2.5 2.5 0 0 0 117.5 30 H 100')
                            // Paper
                            path.error-img-paper(d='M20 50 V 0 H 100 V 50 Z')
                            // Lines
                            path.error-img-line(d='M30 10 H 90')
                            path.error-img-line(d='M30 10 H 90')
                            path.error-img-line(d='M30 15 H 90')
                            path.error-img-line(d='M30 20 H 90')
                            path.error-img-line(d='M30 25 H 90')
                            path.error-img-line(d='M30 30 H 90')
                            path.error-img-line(d='M30 35 H 90')
                            path.error-img-line(d='M30 40 H 90')
                            // Bottom
                            path.error-img-bottom(d='M10 80 V 87.5 A 2.5 2.5 0 0 0 12.5 90 H 107.5 A 2.5 2.5 0 0 0 110 87.5 V 80')
                            // Buttons
                            circle.error-img-light.error-img-light-success(cx='95.5' cy='65' r='2.5')
                            circle.error-img-light.error-img-light-warning(cx='101.5' cy='65' r='2.5')
                            circle.error-img-light.error-img-light-danger(cx='107.5' cy='65' r='2.5')
                            // Stripes
                            path.error-img-stripe(d='M15 90 V 120')
                            path.error-img-stripe(d='M20 90 V 120')
                            path.error-img-stripe(d='M25 90 V 120')
                            path.error-img-stripe(d='M30 90 V 120')
                            path.error-img-stripe(d='M35 90 V 120')
                            path.error-img-stripe(d='M40 90 V 120')
                            path.error-img-stripe(d='M45 90 V 120')
                            path.error-img-stripe(d='M50 90 V 120')
                            path.error-img-stripe(d='M55 90 V 120')
                            path.error-img-stripe(d='M60 90 V 120')
                            path.error-img-stripe(d='M65 90 V 120')
                            path.error-img-stripe(d='M70 90 V 120')
                            path.error-img-stripe(d='M75 90 V 120')
                            path.error-img-stripe(d='M80 90 V 120')
                            path.error-img-stripe(d='M85 90 V 120')
                            path.error-img-stripe(d='M90 90 V 120')
                            path.error-img-stripe(d='M95 90 V 120')
                            path.error-img-stripe(d='M100 90 V 120')
                            path.error-img-stripe(d='M105 90 V 120')
                .col-md-2.order-md-1
                    // Title
                    h6.title
                        | 404 error page
                .col-md-5.order-md-2
                    // Heading
                    h2.mb-4
                        | Page 
                        span.text-primary not
                        |  found
                    // Subheading
                    p.text-muted
                        | The page you are looking for is not here. It has either been moved or never existed. Please go to homepage and start from there instead.
                    // Button
                    a.btn.btn-outline-primary(href='index.html')
                        | Homepage 
                        i.fas.fa-angle-right.ml-1

    +footer()   
